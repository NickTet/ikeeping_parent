<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zz.ikeeping.service.login.dao.OrdersStatusDao">
    <select id="selectByStatus" parameterType="map" resultType="com.zz.ikeeping.entity.Order">
        SELECT * FROM t_order WHERE status=#{status} AND uid=#{uid}
    </select>

</mapper>